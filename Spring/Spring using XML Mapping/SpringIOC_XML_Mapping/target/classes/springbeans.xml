<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
        "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
        
        
<beans>
	<bean id="emp" class="com.entity.EmployeeEg">
	<property name="empid" value="12"></property>	
	<property name="empname" value="Ramesh"></property>	
	<property name="dept" value="Office boy"></property>	
	<property name="salary" value="2LPA"></property>	
	<property name="con" ref="c"></property>	
	</bean>
	
	<bean id="c" class="com.entity.Country">
	<property name="ccode" value="91"></property>
	<property name="country" value="India"></property>	
	
	</bean>
</beans>